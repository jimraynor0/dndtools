<Configuration status="info">
	<Loggers>
		<Logger name="org.toj.dnd.irctoolkit.mapgenerator" level="debug"/>
		<Logger name="org.toj.dnd.irctoolkit.ui.map.mappane.SelectionCompleteListener" level="info"/>
		<Root level="debug">
			<AppenderRef ref="Console" />
		</Root>
	</Loggers>
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} %-5p [%t] %c %3x - %m%n" />
		</Console>
	</Appenders>
</Configuration>
